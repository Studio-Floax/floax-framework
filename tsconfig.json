{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@types/node"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@floax/components": [
        "dist/floax/components",
        "dist/floax/components"
      ],
      "@floax/components/*": [
        "dist/floax/components/*",
        "dist/floax/components/*"
      ],
      "floax-components": [
        "dist/floax-components"
      ],
      "floax-components/*": [
        "dist/floax-components/*"
      ],
      "components": [
        "dist/components"
      ],
      "components/*": [
        "dist/components/*"
      ],
      "services": [
        "dist/services"
      ],
      "services/*": [
        "dist/services/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}